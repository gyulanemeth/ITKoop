JSON üzenetek formátuma

 {
        "type":
        "sender":
        "message": {
                //ez type függő
        }
 }

 type == 2 -> objektum mozgatása
 message: {
        "objId":
        "x":
        "y":
        "savePos":
 }

 type == 3 -> objektum létrehozása
 message: {
        exampleString
        x
        y
        z
 }

 type == 4 -> objektum törlése
 message: {
        objectId
 }

 type == 5 -> objektum módosítása
 message: {
        objectId
        exampleString
 }


 type == 1000 -> chat message
 message: {
        "userName":
        "msg":
        "timeStamp":
 }


 Az adatbázisban lévő objektumok:
 objektum: {
        _id
        pos: {
                x
                y
                z
        },
        data: {
                exampleString
        }
 }